{
  "name": "backlog-trello-sync",
  "version": "1.0.0",
  "description": "Synchronize your backlog cards with a Trello board",
  "main": "index.js",
  "scripts": {
    "test": "ava tests",
    "test:watch": "ava tests --watch",
    "lint": "eslint src tests",
    "lintfix": "npm run lint -- --fix",
    "build": "babel src -d dist",
    "start": "npm run build && NODE_TLS_REJECT_UNAUTHORIZED=0 node dist",
    "dev": "nodemon --exec npm run start"
  },
  "keywords": [
    "backlogapp",
    "backlog",
    "trello"
  ],
  "author": "Vlad Barboni <vlad.barboni@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-plugin-espower": "2.3.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "3.0.1",
    "eslint-config-airbnb-base": "4.0.0",
    "eslint-plugin-import": "1.10.2",
    "nodemon": "1.10.0"
  },
  "dependencies": {
    "backlog-models": "1.0.1",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "convict": "1.4.0",
    "cors": "2.8.1",
    "express": "4.14.0",
    "meteor-random": "0.0.3",
    "oauth": "0.9.14"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
